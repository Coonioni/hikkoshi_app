

<%= form_with model: @post, local: true do |form| %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :post_content %>
  <%= form.text_area :post_content %>

<%= form.collection_select :tag_id, Tag.order(:tag_name), :id, :tag_name %>

  <%= form.submit %>

<% end %>